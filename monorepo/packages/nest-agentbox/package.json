{
  "name": "@dbc-tech/nest-agentbox",
  "version": "1.0.2",
  "description": "AgentBox Nest integration",
  "author": "DBC",
  "private": false,
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
